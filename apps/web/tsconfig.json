{
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"]
        },
        "allowJs": true,
        "plugins": [
            {
                "name": "@astrojs/ts-plugin"
            }
        ],
        "jsx": "react"
    }
}
